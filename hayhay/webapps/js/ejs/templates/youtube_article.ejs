<% for(i=0;i<news.length;i++){ 
	try{
%>
	<div class="row" style="padding-left: 8px;">
		<div class="col-xs-12 col-sm-12 col-md-12 col-lg-12" style="padding: 0px;border-top: 1px solid #8c8c8c !important;padding-top: 10px;">
			<div class="container-fluid" style="padding: 0px 4px 2px 0px;">
				<div class="row" style="padding-left: 8px;padding-bottom: 12px;">
					<div class="col-xs-4 col-sm-4 col-md-4 col-lg-4" style="padding: 0px;" id="<%=news[i].id%>">
						<div class="container-fluid" style="padding: 0px 4px 2px 0px;">
							<a  onclick="return openYoubuteArticle('<%=news[i].url%>');">
								<img class="img-responsive" style="width: 100%;" alt="${news[i].title}" src="<%=news[i].imageUrl%>">
							</a>
						</div>
					</div>
					<div class="col-xs-8 col-sm-8 col-md-8 col-lg-8" style="padding: 0px;">
						<div class="container-fluid" style="padding: 0px 4px 2px 0px;">
							<a  onclick="return openYoubuteArticle('<%=news[i].url%>');" style="color:black;text-decoration: none;"><%=news[i].title%></a>
							<span class="website_info">(<%=news[i].fromWebSite%>  <%=news[i].strDate%>)</span>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
<% 
}catch(e){
	console.log(e+'==================');
}
}%>


	

